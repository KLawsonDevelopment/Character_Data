<form action='/' , method="POST">
    <h2>New Game</h2>
    <div>
        <label for="title">Title</label>
        <input type="text" name="title" id="title" placeholder="Game Title">
    </div>
    <div>
        <label for="year">Year Released</label>
        <input type="number" max=2019 name="year" id="year" placeholder="Year of Release">
    </div>
    <div>
        <label for="company">Company</label>
        <input type="text" name="company" id="company" placeholder="Publishing Company">
    </div>
    <div>
        <label for="imgLink">Logo</label>
        <input type="text" name="imgLink" id="imgLink" placeholder="Company Logo">
    </div>
    <input type="submit" value="submit">
</form>